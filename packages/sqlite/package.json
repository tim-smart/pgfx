{
  "name": "@sqlfx/sqlite",
  "version": "0.41.0",
  "description": "",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/tim-smart/sqlfx.git"
  },
  "author": "Tim Smart <hello@timsmart.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tim-smart/sqlfx/issues"
  },
  "homepage": "https://github.com/tim-smart/sqlfx",
  "scripts": {
    "build": "pnpm build-esm && pnpm build-cjs && pnpm build-annotate && build-utils pack-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build --plugins annotate-pure-calls --out-dir build --source-maps"
  },
  "effect": {
    "generateExports": {
      "include": [
        "*.ts",
        "Migrator/*.ts"
      ]
    }
  },
  "keywords": [],
  "sideEffects": false,
  "dependencies": {
    "@sqlfx/sql": "workspace:^"
  },
  "devDependencies": {
    "@effect/schema": "^0.60.2",
    "@sqlite.org/sqlite-wasm": "3.44.2-build3",
    "@types/better-sqlite3": "^7.6.8",
    "better-sqlite3": "^9.2.2",
    "bun-types": "^1.0.22",
    "effect": "^2.1.0",
    "expo-sqlite": "^11.6.0",
    "react-native-quick-sqlite": "^8.0.6"
  },
  "peerDependencies": {
    "@sqlite.org/sqlite-wasm": "3.44.2-build3",
    "better-sqlite3": "^9",
    "effect": "^2.1.0",
    "expo-sqlite": "^11",
    "react-native-quick-sqlite": "^8"
  },
  "peerDependenciesMeta": {
    "@sqlite.org/sqlite-wasm": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    },
    "expo-sqlite": {
      "optional": true
    },
    "react-native-quick-sqlite": {
      "optional": true
    }
  }
}
